코딩테스트 공배수 
이진수 로 바구기 연습

#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int solution(int num, int n, int m) {
    if (num % n == 0 && num % m == 0) {
        return 1;
    } else {
        return 0;
    }
}

int main() {
    int num, n, m;
    scanf("%d %d %d", &num, &n, &m);
    int result = solution(num, n, m);
    printf("%d", result);
    return 0;
}
